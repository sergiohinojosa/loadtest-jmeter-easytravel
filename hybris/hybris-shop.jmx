        <BeanShellPreProcessor guiclass="TestBeanGUI" testclass="BeanShellPreProcessor" testname="dynatrace Tagging" enabled="true">
          <boolProp name="resetInterpreter">false</boolProp>
          <stringProp name="parameters"></stringProp>
          <stringProp name="filename"></stringProp>
          <stringProp name="script">import org.apache.jmeter.util.JMeterUtils;

StringBuilder headerValue = new StringBuilder();

String headerKey = &quot;X-dynaTrace&quot;;

// Do not clear the whole header, it&apos;ll mess up subsequent headers if you have
// different requests like partial and full requests.
// sampler.getHeaderManager().clear();

// Just clearing the X-dynaTrace Variable.
sampler.getHeaderManager().removeHeaderNamed(headerKey);


String trid = JMeterUtils.getProperty(&quot;testrunId&quot;);

String testName = ctx.getThreadGroup().getName();

//RUN_ID is just a TimeStamp
String DATEID = vars.get(&quot;DATE_ID&quot;);

//String packagename=&quot;de.dynatrace.webapitest.&quot;;

//*Timer NAme (Name of the Step e.g. Home Page)
String NA = &quot;NA=&quot; + sampler.getName()+&quot;;&quot;;

// AgentName
String AN = &quot;&quot;;

//ScriptName
String SN = &quot;SN=&quot;+testName+&quot; JMeter;&quot;;

// VirtualUser(in this case is the ThreadNumber)
String VU = &quot;VU=&quot;+ctx.getThreadNum()+&quot;;&quot;;

// TestName
String TE = &quot;TE=&quot;+testName+ &quot;-&quot; + DATEID+&quot;;&quot;;

/* In Dynatrace versions prior to 6.2, it was required 
 * to provide NA and TR fields, with optional RC field. 
 * Starting from version 6.2, TN field should be used to provide 
 * test name. TR and RC fields can still be sed if you want to 
 * provide test run id or the expected response code.
 */
 // Test RunId (from DT Server)
String TR = &quot;&quot;;

//Response code;
String RC = &quot;RC=200;&quot;;

// Geographical Location
String GR = &quot;&quot;;

if(trid != null &amp;&amp; !trid.isEmpty()){
	TR = &quot;TR=&quot;+trid+&quot;;&quot;;
}

headerValue.append(SN);
headerValue.append(NA);
headerValue.append(TE);
headerValue.append(RC);
headerValue.append(AN);
headerValue.append(TR);

sampler.getHeaderManager().add(new org.apache.jmeter.protocol.http.control.Header(headerKey, headerValue.toString()));
</stringProp>
